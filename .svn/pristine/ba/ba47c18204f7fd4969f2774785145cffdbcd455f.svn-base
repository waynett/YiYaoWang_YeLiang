#import <Foundation/Foundation.h>


@interface UserVO : NSObject {
@private
	NSNumber * amount;
	NSNumber * boughtTimes;
	NSString * cocode;
	NSNumber * couponCount;
	NSString * email;
	NSNumber * messageCount;
	NSString * pictureUrl;
	NSNumber * points;
	NSNumber * userId;
	NSString * userMobile;
	NSString * username;
	NSString * userPhone;
	NSString * vip;
    NSNumber * cardAmount;//帐号礼品卡金额
    NSNumber * availableAmount;//账户可用金额
    NSNumber * frozenAmount;//账户冻结金额
    NSString * userRealName;//用户昵称
    
    NSNumber* availableCardAmount; //可使用礼品卡金额
    NSNumber* totalAmount; //账号总余额
    NSNumber* cardNum; //礼品卡数量
    NSNumber* frozenCardAmount; // 冻结礼品卡金额
    NSNumber* enduserPoint;//用户可用积分
}
@property(retain,nonatomic)NSNumber * amount;
@property(retain,nonatomic)NSNumber * boughtTimes;
@property(retain,nonatomic)NSString * cocode;
@property(retain,nonatomic)NSNumber * couponCount;
@property(retain,nonatomic)NSString * email;
@property(retain,nonatomic)NSNumber * messageCount;
@property(retain,nonatomic)NSString * pictureUrl;
@property(retain,nonatomic)NSNumber * points;
@property(retain,nonatomic)NSNumber * userId;
@property(retain,nonatomic)NSString * userMobile;
@property(retain,nonatomic)NSString * username;
@property(retain,nonatomic)NSString * userPhone;
@property(retain,nonatomic)NSString * vip;
@property(retain,nonatomic)NSNumber * cardAmount;
@property(retain,nonatomic)NSNumber * availableAmount;//账户可用金额
@property(retain,nonatomic)NSNumber * frozenAmount;//账户冻结金额
@property(retain,nonatomic)NSString * userRealName;//用户昵称
@property(retain,nonatomic)NSNumber* availableCardAmount; //可使用礼品卡金额
@property(retain,nonatomic)NSNumber* totalAmount; //账号总余额
@property(retain,nonatomic)NSNumber* cardNum; //礼品卡数量
@property(retain,nonatomic)NSNumber* frozenCardAmount;
@property(retain,nonatomic)NSNumber* enduserPoint;
@end
