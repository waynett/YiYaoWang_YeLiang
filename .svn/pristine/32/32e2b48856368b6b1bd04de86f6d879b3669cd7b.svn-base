    }
    if (siteType!=nil) {
        [mDictionary setObject:siteType forKey:@"siteType"];
    }
    if (grouponSerialVO!=nil) {
        [mDictionary setObject:[grouponSerialVO dictionaryFromVO] forKey:@"grouponSerialVO"];
    }
    if (grouponSerials!=nil && [grouponSerials count]>0) {
        NSMutableArray *mArray=[[[NSMutableArray alloc] init] autorelease];
        for (GrouponSerialVO *vo in grouponSerials) {
            [mArray addObject:[vo dictionaryFromVO]];
        }
        [mDictionary setObject:mArray forKey:@"grouponSerials"];
    }
    if (colorList!=nil) {
