    }
    if (mallURL!=nil) {
        [mDictionary setObject:sizeList forKey:@"mallURL"];
    }
    if (productVO!=nil) {
        [mDictionary setObject:[productVO dictionaryFromVO] forKey:@"productVO"];
    }
    return mDictionary;
}

